User
    email Text
    password Text Maybe
    name Text Maybe
    isAdmin Bool
    UniqueUser email
    deriving Typeable
    deriving Show
Room
    name Text
    deriving Show
RoomAccess
    userId UserId
    roomId RoomId
    lastSeenAt UTCTime default=now()
    deriving Show
Message
    roomId RoomId
    username Text
    type Text
    value Text
    createdAt UTCTime default=now()
    deriving Show
