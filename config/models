User
    email Text
    password Text Maybe
    UniqueUser email
    deriving Typeable
    deriving Show
Room
    name Text
    deriving Show
RoomAccess
    userId UserId
    roomId RoomId
    lastSeenAt UTCTime default=CURRENT_TIME
    deriving Show
Message
    roomId RoomId
    username Text
    type Text
    value Text
    createdAt UTCTime default=CURRENT_TIME
    deriving Show
